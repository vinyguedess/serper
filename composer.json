{
    "name": "vinyguedess/serper",
    "description": "Spider through SERPs checking results",
    "version": "1.0.0",
    
    "autoload": {
        "psr-4": {
            "SERPer\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SERPer\\Test\\": "test/"
        }
    },

    "require": {
        "php": "^7.1",
        "silex/silex": "^2.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "php": "^7.1",
        "phpunit/phpunit": "^6.4",
        "symfony/browser-kit": "^3.3",
        "symfony/css-selector": "^3.3"
    },

    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml --coverage-html coverage"   
    }
}
